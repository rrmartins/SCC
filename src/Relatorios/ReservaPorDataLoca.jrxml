	<queryString>
		<![CDATA[SELECT
     re.*,
     cl."nome",
     re."concluida" AS re_concluida
FROM
     "reserva" re INNER JOIN "cliente" cl ON re."cod_cliente" = cl."cod_cliente"
WHERE
     re.data_locacao BETWEEN $P{data_inicio} AND $P{data_fim}
ORDER BY
     data_locacao ASC;]]>
	</queryString>
