	<queryString>
		<![CDATA[SELECT
     cl."nome",
     cl."cpf",
     ca."placa",
     ca."modelo",
     lo."quilometragem_prevista"
FROM
     "carro" ca INNER JOIN "locacao" lo ON ca."cod_carro" = lo."cod_carro"
     INNER JOIN "cliente" cl ON lo."cod_cliente" = cl."cod_cliente"
WHERE
     lo.data_locacao BETWEEN $P{data_inicio} AND $P{data_fim};]]>
	</queryString>
